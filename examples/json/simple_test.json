                       "range_dim2": [-100, 100]}
      },
      {"task":  "clustering_data",
       "type": "KMmeans",
       "parameters":  {"k": 10}
      },
      {"task":  "binning_scores",
       "type": "binning",
       "parameters":  {"num_bins": 4}
      },
      {
        "task": "write_out",
