{
  "chemcharts": {
    "header": {},
    "execution": [
      {"task":  "data_loading",
       "input":  ["data/scaffold_memory.csv", "data/scaffold_memory_run2.csv"],
       "input_type":  "csv",
       "columns":  {
                     "smiles_column": "SMILES",
                     "scores_column": "total_score",
                     "epochs_column": "Step"
                    }
      },
      {"task":  "generate_fingerprints",
       "type":  "maccs",
       "parameters":  {"useFeatures":  true}
      },
      {"task":  "dimensional_reduction",
       "type": "UMAP",
       "parameters":  {}
      },
      {"task":  "clustering_data",
       "type": "KMmeans",
       "parameters":  {"k": 30}
      },
      {"task":  "binning_scores",
       "type": "binning",
       "parameters":  {"num_bins": 10}
      },
      {"task": "generate_plot",
       "type": "trisurf_static_plot",
       "parameters":  {"title": "Trisurf Static ChemCharts Plot",
                       "color": "gist_rainbow"},
        "settings": {"path": "output/plots/trisurf_static_plot.png",
                     "format": "png",
                     "dpi": 150,
                     "figsize": [9, 9]}
      }
    ]
  }
}